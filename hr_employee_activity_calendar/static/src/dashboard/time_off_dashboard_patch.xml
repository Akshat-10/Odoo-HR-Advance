<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="hr_employee_activity.TimeOffDashboardPatch" t-inherit="hr_holidays.TimeOffDashboard" t-inherit-mode="extension">
        <xpath expr="//div[contains(@class, 'o_timeoff_dashboard')]" position="inside">
            <EmployeeMetrics
                employeeId="props.employeeId"
                date="state.date"
                rangeStart="props.rangeStart"
                rangeEnd="props.rangeEnd"
                scale="props.scale"
            />
        </xpath>
    </t>
</templates>
