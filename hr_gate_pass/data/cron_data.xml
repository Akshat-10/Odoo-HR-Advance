<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    <record id="ir_cron_gatepass_overdue" model="ir.cron">
            <field name="name">GatePass Overdue Check</field>
            <field name="model_id" ref="model_hr_gate_pass"/>
            <field name="state">code</field>
            <field name="code">model._cron_check_overdue_returns()</field>
            <field name="interval_number" eval="1"/>
